<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="none" name="NopCommerce UI - Automation Testing">
    <parameter name="userUrl" value="https://demo.nopcommerce.com/" />
    <parameter name="adminUrl" value="https://admin-demo.nopcommerce.com/" />
    <listeners>
        <listener class-name="commons.MethodListener" />
        <listener class-name="reportConfigs.AllureTestListener" />
    </listeners>
    <test name="Run on Firefox browser">
        <parameter name="browser" value="Firefox" />
        <classes>
            <class name="com.nopcommerce.users.Lever_19_Allure_Report" />
        </classes>
    </test>
</suite>